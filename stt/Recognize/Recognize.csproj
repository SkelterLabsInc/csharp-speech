<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.8.0" />
    <PackageReference Include="Google.Protobuf" Version="3.18.0" />
    <PackageReference Include="Grpc.Net.Client" Version="2.39.0" />
    <PackageReference Include="Grpc.Net.ClientFactory" Version="2.32.0" />
    <PackageReference Include="Grpc.Tools" Version="2.40.0">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="NAudio" Version="2.1.0" />

    <!-- Generated by
      $ dotnet-grpc add-file -i ../Protos ../Protos/**/*.proto-->
    <Protobuf Include="..\Protos\google\api\annotations.proto" GrpcServices="Client" AdditionalImportDirs="..\Protos" Link="Protos\annotations.proto" />
    <Protobuf Include="..\Protos\google\api\http.proto" GrpcServices="Client" AdditionalImportDirs="..\Protos" Link="Protos\http.proto" />
    <Protobuf Include="..\Protos\google\api\label.proto" GrpcServices="Client" AdditionalImportDirs="..\Protos" Link="Protos\label.proto" />
    <Protobuf Include="..\Protos\google\api\monitored_resource.proto" GrpcServices="Client" AdditionalImportDirs="..\Protos" Link="Protos\monitored_resource.proto" />
    <Protobuf Include="..\Protos\google\longrunning\operations.proto" GrpcServices="Client" AdditionalImportDirs="..\Protos" Link="Protos\operations.proto" />
    <Protobuf Include="..\Protos\google\protobuf\descriptor.proto" GrpcServices="Client" AdditionalImportDirs="..\Protos" Link="Protos\descriptor.proto" />
    <Protobuf Include="..\Protos\google\protobuf\duration.proto" GrpcServices="Client" AdditionalImportDirs="..\Protos" Link="Protos\duration.proto" />
    <Protobuf Include="..\Protos\google\protobuf\empty.proto" GrpcServices="Client" AdditionalImportDirs="..\Protos" Link="Protos\empty.proto" />
    <Protobuf Include="..\Protos\google\protobuf\timestamp.proto" GrpcServices="Client" AdditionalImportDirs="..\Protos" Link="Protos\timestamp.proto" />
    <Protobuf Include="..\Protos\google\rpc\code.proto" GrpcServices="Client" AdditionalImportDirs="..\Protos" Link="Protos\code.proto" />
    <Protobuf Include="..\Protos\google\rpc\error_details.proto" GrpcServices="Client" AdditionalImportDirs="..\Protos" Link="Protos\error_details.proto" />
    <Protobuf Include="..\Protos\google\rpc\status.proto" GrpcServices="Client" AdditionalImportDirs="..\Protos" Link="Protos\status.proto" />
    <Protobuf Include="..\Protos\google\speech\v1\cloud_speech.proto" GrpcServices="Client" AdditionalImportDirs="..\Protos" Link="Protos\cloud_speech.proto" />
  </ItemGroup>
</Project>
